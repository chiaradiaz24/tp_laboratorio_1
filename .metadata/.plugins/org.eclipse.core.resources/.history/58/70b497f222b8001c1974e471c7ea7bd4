int calcularPromedioPositivo (int numero)
{
	int retorno;
	int suma;
	int contador;
	int acumulador;


	contador=0;
	acumulador=0;


	if(numero>0)
		{
			contador++;
			acumulador+= numero;
			promedio=acumulador/contador;


			retorno= promedio;


		}



	return retorno;



	}

